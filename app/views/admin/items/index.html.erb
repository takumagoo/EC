<h1>Admin::Items#index</h1>
<p>Find me in app/views/admin/items/index.html.erb</p>

<% @items.each do |item| %>

  <!--商品ID-->
  <p>商品ID</p>
  <%= item.id %>
  <!--商品名-->
  <p>商品名</p>
  <%= item.name %>
  <!--税抜き価格-->
  <p>税抜き価格</p>
  <%= item.price %>
  <!--ジャンル-->
  <p>ジャンル</p>
  <%= item.genre.name %>
  <!--ステータス-->
  <p>ステータス</p>
  <% if item.is_active %>
    販売中
  <% else%>
    販売停止中
  <% end %>
<% end %>