<h1>Admin::Customers#index</h1>
<p>Find me in app/views/admin/customers/index.html.erb</p>

<% @customers.each do |customer| %>
  <!--会員ID-->
  <p>会員ID</p>
  <%= customer.id %>
  <!--商品名-->
  <p>氏名</p>
  <%= customer.last_name %>
  <%= customer.first_name %>
  <!--メールアドレス-->
  <p>メールアドレス</p>
  <%= customer.email %>
  <!--ステータス-->
  <p>ステータス</p>
  <% if customer.is_deleted %>
    販売中
  <% else %>
    販売停止中
  <% end %>
<% end %>
