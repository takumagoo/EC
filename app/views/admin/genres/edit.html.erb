<p id="notice"></p>
<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-12">
      <h4 class="bg-secondary text-dark d-inline-block">ジャンル編集</h4>
      <div class="row">
        <%= form_with model: @genre, url:admin_genre_path, method: :patch do |f| %>
          <div class="col-5 pb-3">
            <p>ジャンル名</p>
          </div>
          <div class="col-5 pb-3">
            <%= f.text_field :name %>
          </div>
          <div class="col-5 pb-3">
             <%= f.submit '変更を保存', class: "btn btn-success" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>


